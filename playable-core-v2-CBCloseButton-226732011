function _interopRequireDefault(obj) { return obj && obj.__esModule ? obj : { default: obj }; }

function _interopRequireWildcard(obj) { if (obj && obj.__esModule) { return obj; } else { var newObj = {}; if (obj != null) { for (var key in obj) { if (Object.prototype.hasOwnProperty.call(obj, key)) newObj[key] = obj[key]; } } newObj.default = obj; return newObj; } }

function requestUrl(method, url, body) {
  var headers = arguments.length > 3 && arguments[3] !== undefined ? arguments[3] : {};

  var xhr = new XMLHttpRequest();
  xhr.open(method, url, true);
  Object.keys(headers).forEach(function (key) {
    xhr.setRequestHeader(key, headers[key]);
  });
  xhr.send(body);
  return xhr;
}

function reportSentryError(message, error) {
  var level = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : 'error';

  Sentry.withScope(function (scope) {
    scope.setLevel(level);
    if (error) {
      Sentry.addBreadcrumb({
        category: 'Manual',
        level: Sentry.Severity.Warning,
        data: {
          error: error
        }
      });
    }
    Sentry.captureMessage(message);
  });
}

function roundToXDecimal(number, x) {
  if (x < 0) x = 0;
  // eslint-disable-next-line no-restricted-properties
  var powerOfTen = Math.pow(10, x);
  return Math.round(number * powerOfTen) / powerOfTen;
}

function getStarClassFromStars(stars) {
  var jsonStarClassMap = {
    0: 'zero-star',
    0.5: 'half-star',
    1: 'one-star',
    1.5: 'one-half-star',
    2: 'two-star',
    2.5: 'two-half-star',
    3: 'three-star',
    3.5: 'three-half-star',
    4: 'four-star',
    4.5: 'four-half-star',
    5: 'five-star'
  };

  // making sure that stars variables
  // is not a "0", null, false, undefined, and any other string.
  var numStarRating = 4;
  if (+stars) {
    numStarRating = roundToXDecimal(parseFloat(stars), 1);
  }

  for (var j = 0; j < 5; j += 0.5) {
    if (j >= numStarRating) {
      return jsonStarClassMap[j];
    }
  }
  return jsonStarClassMap[5];
}

function dpiToString() {
  var dpi = 'normal';
  if (window.devicePixelRatio >= 1.5) {
    dpi = 'high';
  } else if (window.devicePixelRatio <= 0.75) {
    dpi = 'low';
  }
  return dpi;
}

function getIosVersion() {
  var userAgent = navigator.userAgent.toLowerCase();
  var match = userAgent.match(/os\s([0-9_]*)/);
  return match ? match[1].replace(/_/g, '.') : false;
}

function getAndroidVersion() {
  var userAgent = navigator.userAgent.toLowerCase();
  var match = userAgent.match(/android\s([0-9.]*)/);
  return match ? match[1] : false;
}

function getOsVersion(osVersion) {
  var version = void 0;
  if (osVersion === _Constants2.default.ANDROID) {
    version = 'Android ' + getAndroidVersion();
  } else if (osVersion === _Constants2.default.IOS) {
    version = 'iOS ' + getIosVersion();
  } else {
    version = 'Undefined';
  }

  return version;
}

function getScreenDimensions(param) {
  var screen = {
    screenWidth: window.innerWidth,
    screenHeight: window.innerHeight
  };

  return screen[param] ? screen[param] : screen;
}

function isInterstitial(adType) {
  var interstitialType = [1, 5, 6, 8];
  return interstitialType.indexOf(adType) !== -1;
}

function isRewarded(adType) {
  var rewardType = [4, 9];
  return rewardType.indexOf(adType) !== -1;
}

function isStatic(adType) {
  var staticType = [1];
  return staticType.indexOf(adType) !== -1;
}

function formatPlayerNumber(num) {
  if (num >= 1000000000) {
    return Math.floor(num / 1000000000) + ' Billion';
  } else if (num >= 1000000) {
    return Math.floor(num / 1000000) + ' Million';
  } else if (num >= 100000 && num <= 999999) {
    return String(Math.floor(num - num % 100000)).replace(/(\d)(?=(\d{3})+$)/g, '$1,');
  } else if (num >= 10000 && num <= 99999) {
    return String(Math.floor(num - num % 10000)).replace(/(\d)(?=(\d{3})+$)/g, '$1,');
  }
  return '0';
}

function formatInstallString(installs) {
  var numberInstallsPhrase = '';
  if (!isNaN(installs)) {
    var installsInt = parseInt(installs, 10);
    if (installsInt >= 10000) {
      var formattedNum = formatPlayerNumber(installsInt);
      if (formattedNum !== '0') {
        numberInstallsPhrase = formattedNum + '+ Players';
      }
    }
  }

  return numberInstallsPhrase;
}

function formatNumber(number) {
  var sign = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : ',';

  return number.toString().replace(/\B(?=(\d{3})+(?!\d))/g, sign);
}

function getPlatform() {
  var userAgent = window.navigator.userAgent || window.navigator.vendor || window.opera;

  if (userAgent.match(/iPad/i) || userAgent.match(/iPhone/i) || userAgent.match(/iPod/i)) {
    return _Constants2.default.IOS;
  } else if (userAgent.match(/Android/i)) {
    return _Constants2.default.ANDROID;
  }
  return _Constants2.default.IOS;
}

function androidNativeBridge(eventType, eventArgs) {
  if (typeof eventArgs === 'undefined') eventArgs = {};
  window.prompt(JSON.stringify({
    eventType: eventType,
    eventArgs: eventArgs
  }));
}

function getUrlForEvent(eventType, eventArgs) {
  var eventUrl = 'cbsdk://eventHandler/eventType/' + eventType + '/';
  if (eventArgs) {
    eventUrl += 'eventArgs/';
    for (var argName in eventArgs) {
      if (eventArgs.hasOwnProperty(argName)) {
        eventUrl += argName + '/' + encodeURIComponent(eventArgs[argName]) + '/';
      }
    }
  }
  return eventUrl;
}

function iosCallNative(eventType, eventArgs, urlOverride) {
  var url = urlOverride || getUrlForEvent(eventType, eventArgs);
  var rootElm = document.documentElement;
  var newFrameElm = document.createElement('iframe');
  newFrameElm.setAttribute('src', url);
  rootElm.appendChild(newFrameElm);
  newFrameElm.parentNode.removeChild(newFrameElm);
}

function callNative(platform, eventType, eventArgs, urlOverride) {
  if (platform === _Constants2.default.ANDROID) {
    console.log('callNative', eventArgs, eventType);
    androidNativeBridge(eventType, eventArgs);
  } else {
    iosCallNative(eventType, eventArgs, urlOverride);
  }
}

function isVariableReplaced(value) {
  return !/^{[{%].+[}%]}$/.test(value);
}

function imgPath(img) {
  var images = 'images';
  return img ? images + '/' + img : null;
}

function createRequest(method, url) {
  var headers = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : {};

  var xhr = new XMLHttpRequest();
  xhr.open(method, url, true);
  Object.keys(headers).forEach(function (key) {
    xhr.setRequestHeader(key, headers[key]);
  });
  return xhr;
}

function getCDNAddress(geo) {
  var cdns = {
    cn: 'https://t.chartboost-china.com/',
    default: 'https://t3.chartboost.com/'
  };

  if (cdns[geo.toLowerCase()]) {
    return cdns[geo.toLowerCase()];
  }
  return cdns.default;
}

function getScreenPercentage(event) {
  var x = null;
  var y = null;
  if (event.x && event.y) {
    x = event.x;
    y = event.y;
  } else if (event.clientX && event.clientY) {
    x = event.clientX;
    y = event.clientY;
  }
  if (x && y) {
    return {
      x: x * 100 / window.innerWidth,
      y: y * 100 / window.innerHeight
    };
  }
  return null;
}

function shouldEnableFeature(percentage) {
  var rand = Math.floor(Math.random() * 100) + 1;
  return rand <= percentage;
}

function setColors(strSelectors) {
  var defaultCountryCode = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : false;

  var objColorsMap = ['grapefruit', // default China Red Button
  'sky', 'sun', 'lime'];
  var intRandomNum = defaultCountryCode ? 0 : Math.floor(Math.random() * objColorsMap.length);
  var strRandomColor = objColorsMap[intRandomNum];

  var htmlColoredElements = document.querySelectorAll(strSelectors);
  for (var i = 0, l = htmlColoredElements.length; i < l; i++) {
    htmlColoredElements[i].classList.add(strRandomColor);
  }

  return strRandomColor;
}

function toggleMute(node, muted) {
  // search for audio elements within the iframe
  // for each audio element,(video, audio) attempt to mute it
  var videoEls = node.getElementsByTagName('video');
  for (var i = 0; i < videoEls.length; i += 1) {
    videoEls[i].muted = muted;
  }

  var audioEls = node.getElementsByTagName('audio');
  for (var j = 0; j < audioEls.length; j += 1) {
    audioEls[j].muted = muted;
  }
}

function mute(node) {
  toggleMute(node, true);
}

function unmute(node) {
  toggleMute(node, false);
}

function atobUtf8(str) {
  return decodeURIComponent(Array.prototype.map.call(atob(str.replace(/\s/g, '')), function (c) {
    return '%' + ('00' + c.charCodeAt(0).toString(16)).slice(-2);
  }).join(''));
}

function encodeSpecialUriParams(url, separator) {
  var encodedUrl = '';

  if (url) {
    var splittedUrl = url.split(separator);
    encodedUrl = splittedUrl[0] + separator + encodeURIComponent(splittedUrl[1]);
  }

  return encodedUrl;
}

function base64Decode(str) {
  return atobUtf8(str).replace(/\\n|\\r|\\|\\t|\\b|\\f|\\v/g, '');
}

/* eslint-disable no-useless-concat */
function setupStarRating(stars, starIds) {
  // These imports are required and will be replaced by the images at gulp time
  var fullStarImage = 'data:' + '@@import /src/images/star_denali.png';
  var halfStarImage = 'data:' + '@@import /src/images/halfstar_denali.png';
  var emptyStarImage = 'data:' + '@@import /src/images/emptystar_denali.png';

  var starsLeft = stars;

  for (var i = 0; i < 5; i++) {
    var starElement = document.getElementById(starIds[i]);

    if (starElement) {
      if (starsLeft > 0.5) {
        starElement.src = fullStarImage;
      } else if (starsLeft > 0) {
        starElement.src = halfStarImage;
      } else {
        starElement.src = emptyStarImage;
      }
    }
    starsLeft--;
  }
}
